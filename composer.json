{
  "name": "awsmug/torro-forms-protectors-plus",
  "description": "Additional Protectors (Anti-Spam) for Torro Forms.",
  "version": "1.0.0",
  "license": "GPL-2.0-or-later",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin",
    "extension",
    "torro forms",
    "forms",
    "form builder",
    "anti-spam",
    "spam"
  ],
  "homepage": "https://torro-forms.com",
  "authors": [
    {
      "name": "Sven Wagener",
      "email": "support@awesome.ug",
      "homepage": "https://awesome.ug",
      "role": "Product Lead"
    }
  ],
  "support": {
    "issues": "https://github.com/awsmug/torro-forms-protectors-plus/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">4.8.20 <6.0",
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "^1",
    "wimg/php-compatibility": "^8",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
    "awsmug/wp-devops": "dev-master"
  },
  "scripts":     {
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ]
  }
}
